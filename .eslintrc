{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [],
  "rules": {
    "no-shadow": 0,
    "quotes": ["error", "single"]
  },
  "plugins": ["prettier"],
  "ignorePatterns": ["node_modules/", "dist/"]
}
