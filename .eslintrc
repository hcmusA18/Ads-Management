{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [],
  "rules": {
    "no-shadow": 0,
    "quotes": [
      "error",
      "single"
    ]
  },
  "ignorePatterns": [
    "node_modules/",
    "dist/"
  ]
}
