{
  "env": {
    "es6": true,
    "jquery": true,
    "node": true,
    "browser": true,
    "commonjs": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [],
  "globals": {
    "$": true,
    "process": true
  },
  "rules": {
    "no-shadow": 0,
    "quotes": ["error", "single"]
  },
  "plugins": ["prettier"],
  "ignorePatterns": ["node_modules/", "dist/"]
}
